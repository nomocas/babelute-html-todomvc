{
  "name": "babelute-html-todomvc",
  "version": "0.1.1",
  "description": "todomvc demo for babelute-html",
  "main": "dist/bundles/index.js",
  "module": "dist/bundles/index.mjs",
  "jsnext:main": "dist/bundles/index.mjs",
  "scripts": {
    "prebuild": "eslint src test",
    "build": "npm run bundles-build && uglifyjs public/app.js -m -o public/app.min.js",
    "bundles-build": "rollup -c",
    "bundles-watch": "rollup -c -w",
    "prepublish": "npm run build"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "keywords": [
    "babelute-html",
    "todomvc"
  ],
  "author": "Gilles Coomans",
  "license": "MIT",
  "dependencies": {
    "event-emitter-es6": "^1.1.5",
    "babelute-html-dom-diffing-pragmatics": "^0.3.0",
    "babelute-html-lexicon": "^0.2.3"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "babel-cli": "^6.14.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-istanbul": "^2.0.2",
    "babel-plugin-unassert": "^2.1.2",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-register": "^6.14.0",
    "babelrc-rollup": "^3.0.0",
    "chai": "^3.5.0",
    "cross-env": "^2.0.1",
    "eslint": "^3.15.0",
    "eslint-plugin-import": "^2.2.0",
    "karma": "^6.3.16",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-mocha": "^1.3.0",
    "karma-rollup-preprocessor": "^3.0.3",
    "mocha": "^3.2.0",
    "nyc": "^10.2.0",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-watch": "^3.2.2",
    "uglify-js": "^2.7.5"
  }
}
